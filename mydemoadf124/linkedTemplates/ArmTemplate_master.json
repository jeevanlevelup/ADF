{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mydemoadf124"
		},
		"AmazonS31_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'AmazonS31'"
		},
		"myfirstls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'myfirstls'"
		},
		"mysink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'mysink'"
		},
		"AmazonS31_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIARIPBLGLVSDVM4GMP"
		},
		"mysink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myaccoutforsink.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mydemoadf124_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AmazonS31_secretAccessKey": {
						"value": "[parameters('AmazonS31_secretAccessKey')]"
					},
					"myfirstls_connectionString": {
						"value": "[parameters('myfirstls_connectionString')]"
					},
					"mysink_accountKey": {
						"value": "[parameters('mysink_accountKey')]"
					},
					"AmazonS31_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS31_properties_typeProperties_accessKeyId')]"
					},
					"mysink_properties_typeProperties_url": {
						"value": "[parameters('mysink_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}